{
    "name": "smontiel/responsive-viewer",
    "description": "A simple responsive viewer for Laravel",
    "type": "library",
    "require": {
        "php": ">=7.1.3",
        "laravel/framework": "~5.6|^6.0|^7.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Salvador Montiel",
            "email": "salvadormontiel8@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Smontiel\\ResponsiveViewer\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Smontiel\\ResponsiveViewer\\LaravelResponsiveViewerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
